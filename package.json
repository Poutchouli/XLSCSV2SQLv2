{
  "name": "sql-canvas",
  "version": "1.0.1",
  "description": "A visual SQL environment for CSV/Excel data exploration and SQLite interaction",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t sql-canvas .",
    "docker:run": "docker run -p 3000:80 sql-canvas",
