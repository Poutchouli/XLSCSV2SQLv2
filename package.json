    "excel", 
    "sqlite", 
    "data-visualization", 
    "solidjs", 
    "typescript"
  ],
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "^3.50.1-build1",
    "papaparse": "^5.4.1",
    "solid-js": "^1.8.11",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "@types/xlsx": "^0.0.35",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.10.2"
  }
}
